{
  "name": "react-classify",
  "version": "1.1.0",
  "description": "Classify functional React components to use component lifecycle hooks",
  "main": "lib/classify.js",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rm -rf lib",
    "example": "cd example && webpack-dev-server --content-base public --inline --hot --host 127.0.0.1 --port 8080",
    "prepublish": "npm run clean && npm run build",
    "test": "mocha --compilers js:babel-register test/tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jfairbank/react-classify.git"
  },
  "keywords": [
    "react",
    "functional",
    "reactjs",
    "functional",
    "programming"
  ],
  "author": "Jeremy Fairbank <elpapapollo@gmail.com> (http://jeremyfairbank.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jfairbank/react-classify/issues"
  },
  "homepage": "https://github.com/jfairbank/react-classify#readme",
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.21",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "enzyme": "^1.2.0",
    "mocha": "^2.3.4",
    "react": "^15.2.1",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^15.2.1",
    "react-redux": "^4.0.2",
    "redux": "^3.0.5",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
